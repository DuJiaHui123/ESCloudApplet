/* pages/account/SignIn.wxss */
.login{
  width: 100%;
  height: 100vh;
}
.loginfirst-img{
  width: 100%;
  height: 100%;
  position: absolute;
  top: 0;
  left: 0;
  z-index: -9999;
}
.login-content{
  padding: 0 40rpx;
  height: 100%;
}
.login-banner {
  text-align: center;
  width: 65%;
  height: 35%;
  margin: 0rpx auto;
}
.login-banner image{
  width: 100%;
  height: 100%;
  margin-top: 20rpx;
}
.login-account{
  margin-top: 60rpx;
}
.van-cell{
  height: 90rpx !important;
  margin-top: 30rpx;
  border-radius: 10rpx;
}
.login-account button{
  margin-top: 40rpx;
  background-color: #5A8FF2;
  color: white;
  border: none;
  border-radius: 10rpx;
}